			"dialogueParts": []
		},
		"dialogueParts": [{
			"speakerID": 0,
			"text": "Hey Sophie, have you ever tried bookbinding?"
		},
		{
			"speakerID": 1,
			"text": "No, I haven’t."
		},
		{
			"speakerID": 0,
			"text": "You should give it a try, I think you’d like it!\r\n"
		}]
	},
	{
		"mainDialogue": {
			"condition": 1,
			"magnitude": 0,
			"targetEmotion": 1,
